{
  "name": "@dnb-org/blocks-bootstrap5",
  "description": "Bootstrap5-Blocks for Hugo",
  "version": "1.0.37",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "bugs": {
    "url": "https://github.com/dnb-org/blocks/issues"
  },
  "commitlint": {
    "extends": [
      "@dnb-org/commitlint-config"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@dnb-org/browserslist-config": "3.2.8",
    "@dnb-org/commitlint-config": "3.1.0",
    "@dnb-org/eslint-config": "3.2.8",
    "@dnb-org/remark-config": "3.2.0",
    "@dnb-org/standard-version-config": "3.2.0",
    "@dnb-org/stylelint-config": "3.2.0",
    "@dnb-org/textlint-config": "3.2.0",
    "standard-version": "9.3.1"
  },
  "homepage": "https://github.com/dnb-org/blocks/tree/master/bootstrap5",
  "license": "MIT",
  "private": true,
  "scripts": {
    "release": "standard-version --release-as patch -a -t \"bootstrap5/v\" --releaseCommitMessageFormat \"chore(release): bootstrap5/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release-major": "standard-version --release-as major -a -t \"bootstrap5/v\" --releaseCommitMessageFormat \"chore(release): bootstrap5/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release-next": "standard-version --release-as minor -a -t \"bootstrap5/v\" --releaseCommitMessageFormat \"chore(release): bootstrap5/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh"
  }
}
