{
  "name": "@davidsneighbour/blocks-bootstrap",
  "description": "Bootstrap-Blocks for Hugo",
  "version": "1.0.31",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "bugs": {
    "url": "https://github.com/davidsneighbour/blocks/issues"
  },
  "commitlint": {
    "extends": [
      "@davidsneighbour/commitlint-config"
    ]
  },
  "devDependencies": {
    "@davidsneighbour/browserslist-config": "^4.0.1",
    "@davidsneighbour/commitlint-config": "^5.0.12",
    "@davidsneighbour/eslint-config": "^4.0.1",
    "@davidsneighbour/remark-config": "^4.0.1",
    "@davidsneighbour/standard-version-config": "^4.0.1",
    "@davidsneighbour/stylelint-config": "^4.0.1",
    "@davidsneighbour/textlint-config": "^4.0.1"
  },
  "homepage": "https://github.com/davidsneighbour/blocks/tree/main/bootstrap",
  "license": "MIT",
  "private": true,
  "scripts": {
    "release": "standard-version --release-as patch -a -t \"bootstrap/v\" --releaseCommitMessageFormat \"chore(release): bootstrap/v{{currentTag}}\" && ./bin/release/postrelease",
    "release-major": "standard-version --release-as major -a -t \"bootstrap/v\" --releaseCommitMessageFormat \"chore(release): bootstrap/v{{currentTag}}\" && ./bin/release/postrelease",
    "release-next": "standard-version --release-as minor -a -t \"bootstrap/v\" --releaseCommitMessageFormat \"chore(release): bootstrap/v{{currentTag}}\" && ./bin/release/postrelease"
  }
}
