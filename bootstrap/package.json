{
  "name": "@dnb-hugo/blocks-bootstrap",
  "description": "Bootstrap-Blocks for Hugo",
  "version": "1.0.14",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "bugs": {
    "url": "https://github.com/dnb-hugo/blocks/issues"
  },
  "commitlint": {
    "extends": [
      "@dnb-hugo/commitlint-config"
    ]
  },
  "devDependencies": {
    "@dnb-hugo/browserslist-config": "2.2.15",
    "@dnb-hugo/commitlint-config": "2.0.20",
    "@dnb-hugo/eslint-config": "2.0.17",
    "@dnb-hugo/remark-config": "2.0.13",
    "@dnb-hugo/standard-version-config": "2.0.12",
    "@dnb-hugo/stylelint-config": "2.0.26",
    "@dnb-hugo/textlint-config": "2.0.12"
  },
  "homepage": "https://github.com/dnb-hugo/blocks/tree/master/bootstrap",
  "license": "MIT",
  "private": true,
  "scripts": {
    "release": "standard-version --release-as patch -a -t \"bootstrap/v\" --releaseCommitMessageFormat \"chore(release): bootstrap/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release-major": "standard-version --release-as major -a -t \"bootstrap/v\" --releaseCommitMessageFormat \"chore(release): bootstrap/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release-next": "standard-version --release-as minor -a -t \"bootstrap/v\" --releaseCommitMessageFormat \"chore(release): bootstrap/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh"
  }
}
